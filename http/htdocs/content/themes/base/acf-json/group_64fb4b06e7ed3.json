        {
            "key": "field_64fb4ce77fd13",
            "label": "Slider Type",
            "name": "slider_type",
            "aria-label": "",
            "type": "button_group",
            "instructions": "<p>Please select the intended slider usage<\/p>\r\n<p>Single or Double?<br>\r\nThis option determines the number of images revealed on the highest breakpoint.<br>\r\n If you would like a single image revealed select the single option, if you would like 2 images revealed within the slider select the double option.<\/p>\r\n\r\n<p>Card or Custom?<br>\r\nSelect the Card option if you would like to utilize a specific post type for the images within your slider. Select custom if you would like to build your slider with custom images.<\/p>",
            "required": 1,
            "conditional_logic": 0,
            "wrapper": {
                "width": "",
                "class": "",
                "id": ""
            },
            "choices": {
                "card-single": "Card - Single Image",
                "card-double": "Card - Double Image",
                "custom-single-card": "Card - Custom Single",
                "custom-double-card": "Card - Custom Double",
                "custom-single": "Custom Single Image",
                "custom-double": "Custom Double Image"
            },
            "default_value": "",
            "return_format": "value",
            "allow_null": 0,
            "layout": "horizontal"
        },
